{
    "name": "StreamBox",
    "plugin": "plugin.video.streambox",
    "priority": 100,
    "assert": {
        "play_movie": [
            "title",
            "year"
        ],
        "play_episode": [
            "showname",
            "season",
            "episode"
        ],
        "search_movie": [
            "title"
        ],
        "search_episode": [
            "showname"
        ]
    },
    "play_movie": [
        "plugin://plugin.video.streambox/?action=search&ask=1",
        {
            "keyboard": "{title}"
        },
        {
            "title": "(?i)^(\\[.*\\])?.*{title}.*(\\[.*\\])?$",
            "year": "{year}"
        }
    ],
    "play_episode": [
        "plugin://plugin.video.streambox/?action=search&ask=1",
        {
            "keyboard": "{showname}"
        },
        {
            "title": "(?i)^(\\[.*\\])?{showname}(\\[.*\\])?$"
        },
        {
            "return": "true",
            "season": "^{season}$",
            "episode": "^{episode}$"
        },
        {
            "season": "^{season}$"
        },
        {
            "season": "^{season}$",
            "episode": "^{episode}$"
        }
    ],
    "search_movie": [
        "plugin://plugin.video.streambox/?action=search&ask=1",
        {
            "keyboard": "{title}"
        }
    ],
    "search_episode": [
        "plugin://plugin.video.streambox/?action=search&ask=1",
        {
            "keyboard": "{showname} S{season}E{episode}"
        }
    ]
}
